<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
<body>
  <%- include('./partials/nav.ejs') %>
  <h1>Contacts:</h1>
  <ul class="contacts">
    <% if (contacts.length) { %>
      <% contacts.forEach(({ id, name_surname }) => { %>
        <li>
          <a href="<%= id %>"><%= name_surname %></a>
        </li>
      <% }) %>
    <% } %>
  </ul>
</body>
</html>
